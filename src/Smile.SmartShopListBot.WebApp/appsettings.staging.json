{
  "Logger": {
    "Rules": [
      {
        "Logger": "*",
        "MinLevel": "Debug",
        "WriteTo": "GeneralFile"
      },
      {
        "Logger": "Microsoft.Hosting.Lifetime",
        "MaxLevel": "Info",
        "WriteTo": "lifetimeConsole",
        "Final": true
      },
      {
        "Logger": "Microsoft.*",
        "MaxLevel": "Info",
        "Final": true
      },
      {
        "Logger": "System.Net.Http.*",
        "MaxLevel": "Info",
        "Final": true
      }
    ]
  }
}
